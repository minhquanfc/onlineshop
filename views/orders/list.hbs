<div class="datalist">
        <h3>Danh sách</h3>
        <table class="table table-bordered">
            <thead>
            <tr>
                <th>Ảnh</th>
                <th>ID</th>
                <th>Tên</th>
                <th>Số điện thoại</th>
                <th>Địa chỉ</th>
                <th>Tổng tiền</th>
                <th>Trạng thái</th>
                <th>Action</th>
            </tr>
            </thead>
            <tbody>
            {{#each listOrder}}
                <tr>
                    <td><img src="data:image/png;base64," width="100px" style="vertical-align: center"></td>
                    <td scope="row">{{this._id}}</td>
                    <td>{{this.hoTen}}</td>
                    <td>{{this.diaChi}}</td>
                    <td>{{this.diaChi}}</td>
                    <td>{{this.tongTien}}</td>
                    <td>{{this.trangThai}}</td>
                    <td>
                        <a href="/orders/edit/{{this._id}}" class="btn btn-primary" id="btnAction">Edit</a>
                        <a href="/orders/delete/{{this._id}}" onclick="return confirm('Bạn có muốn xóa không?')" id="btnAction" class="btn btn-danger">Del</a>
                    </td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>